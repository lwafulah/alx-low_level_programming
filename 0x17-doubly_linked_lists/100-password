#include <stdio.h>
#include <string.h>

/**
* main - allows the user to enter a password and saves it to a file
* Return: 0 on success
*/

int main(void)
{
	char password[100];
	FILE *file;

	printf("Enter the password: ");
	fgets(password, sizeof(password), stdin);
 	password[strcspn(password, "\n")] = '\0';

	if (strcmp(password, "YOUR_PASSWORD") == 0)
	{
		printf("OK\n");
	}
	else
	{
		printf("Incorrect password\n");
	}
	return (0);
}
